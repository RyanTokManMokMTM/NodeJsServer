                <script>
                    const containers = document.querySelector(".container")
                    const seats = document.querySelectorAll(".row .seat:not(.unavailable)")
                    const user_note = document.getElementById("user_ticket_count")
                    const selected_seat = document.getElementById("selected_id")

                    var ticket_full = 0
                    var ticket_discount = 0
                    var user_seat = {}
                        //get shopping list total ticket
                    '<% for(var key in list) { %>' //if having FT
                    '<% if( key == "FT") {%>'
                    var cook = Cookies.get("FT")
                    var FT_info = JSON.parse(cook)

                    ticket_full = FT_info["Count"]
                    '<% }%>'

                    '<% if( key == "DT") {%>'
                    var cook = Cookies.get("DT")
                    var DT_info = JSON.parse(cook)

                    ticket_discount = DT_info["Count"]
                    '<% }%>'
                    '<% } %>'
                    const total = parseInt(ticket_full) + parseInt(ticket_discount)

                    function ticketcount() {
                        const selected_ticket = document.querySelectorAll(".row .selected")
                        const count = selected_ticket.length
                        user_note.innerHTML = count
                            //  console.log(count)
                    }
                    containers.addEventListener("click", (e) => {
                        const selected_ticket = document.querySelectorAll(".row .selected")
                        if (e.target.classList.contains("seat") && !(e.target.classList.contains("unavailable"))) {
                            if (selected_ticket.length < total) { //user be able to select a seat
                                e.target.classList.toggle("selected") // toggle 
                                var seat_id = e.target.id
                                if (e.target.classList.contains("seat") && !e.target.classList.contains("selected")) {
                                    delete user_seat[seat_id]
                                } else {
                                    user_seat[seat_id] = seat_id
                                }
                                //console.log(user_seat)
                                var str = ""
                                ticketcount()

                                for (key in user_seat) {
                                    //console.log(user_seat[key])
                                    str += user_seat[key] + " "
                                }
                                // console.log(str)
                                // console.log(selected_ticket.length)
                                selected_seat.innerHTML = str
                                    //console.log(selected_ticket.length)
                            } else { //user is already selected all seat
                                e.target.classList.toggle("selected") // toggle 
                                var temp = document.querySelectorAll(".row .selected")
                                if (temp.length < total) {
                                    delete user_seat[e.target.id]
                                } else {
                                    var seat_id = e.target.id //get user selected seat id
                                    var first_seat = Object.keys(user_seat)[0]
                                    var first_seats_id = document.getElementById(first_seat)

                                    if (first_seats_id.classList.contains("seat") && first_seats_id.classList.contains("selected")) { // for all seat selected case
                                        first_seats_id.classList.toggle("selected")
                                        delete user_seat[first_seat]
                                        user_seat[seat_id] = seat_id
                                    }
                                }
                                var str = ""
                                console.log(user_seat)
                                    // ticketcount()
                                for (key in user_seat) {
                                    //console.log(user_seat[key])
                                    str += user_seat[key] + " "
                                }

                                selected_seat.innerHTML = str
                                console.log("test2")
                                ticketcount()
                            }
                        }
                    })
                </script>
