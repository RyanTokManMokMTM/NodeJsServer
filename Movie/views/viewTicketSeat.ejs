            <script>
                function obj_length(obj) {
                    let size = 0
                    for (key in obj) {
                        size++
                    }
                    return size
                }

                function checkseat() {
                    var size = obj_length(user_seat)
                    if (size < total) {
                        //if user still have a seat not yet selected
                        console.log(size)
                        var remind = total - size
                        var str = "You still have " + remind + " ticket that you need to select a seat"
                        alert(str)
                        return false
                    } else {
                        //all seat have selected
                        var inFifteenMinutes = new Date(new Date().getTime() + 15 * 60 * 1000);
                        if (Cookies.get("seat")) {
                            Cookies.remove("seat", {
                                path: "/ticket"
                            })

                        }
                        Cookies.set("seat", JSON.stringify(user_seat), {
                            expires: inFifteenMinutes,
                            path: "/ticket"
                        })
                        return true;
                    }

                }
            </script>
            <div style=" margin-left:80%; ">
                <form action="/ticket/ViewUserTicket" method="GET" id="ticket_form" onsubmit="return checkseat()">
                    <button type="submit " class="btn btn-info btn-lg ">Pay</button>
                </form>
